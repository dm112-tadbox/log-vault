{
  "name": "log-vault",
  "version": "1.0.0",
  "description": "Log vault is a library, intended to use in projects where you are required to collect, store and structure the logs, produced by your nodejs process",
  "main": "dist/index.js",
  "scripts": {
    "start": "babel src --out-dir ./src --source-maps ",
    "build": "babel src --out-dir ./dist --source-maps",
    "watch": "babel src --out-dir ./dist --source-maps --watch",
    "test": "export NODE_ENV=test && mocha --colors -b --exit --timeout 40000 --require @babel/register"
  },
  "keywords": [
    "log",
    "winston",
    "console",
    "capture",
    "graylog",
    "mongo",
    "logfile",
    "output",
    "process.stdout"
  ],
  "author": "dm112@tadbox.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "config": "^3.3.9",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "ip": "^2.0.0",
    "mongodb": "^5.6.0",
    "nodemailer": "^6.9.3",
    "sha256": "^0.2.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.22.5",
    "@babel/register": "^7.6.2",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
    "babel-preset-env": "^1.4.0",
    "babel-watch": "^7.7.2",
    "capture-console": "^1.0.1",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "express": "^4.18.2",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "winston-mongodb": "^5.1.1"
  }
}
