{
  "name": "log-vault",
  "version": "1.1.12",
  "description": "Log vault is a library, intended to use in projects where you are required to collect, store and structure the logs, produced by your nodejs process",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "babel src --out-dir ./dist --source-maps --watch",
    "test": "jest --forceExit --verbose --runInBand",
    "test:watch": "jest --watch --forceExit --verbose --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "eslint --fix . --ext .ts",
    "prepublish": "tsc"
  },
  "keywords": [
    "log",
    "winston",
    "console",
    "capture",
    "graylog",
    "mongo",
    "logfile",
    "output",
    "process.stdout"
  ],
  "author": "dm112@tadbox.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "bullmq": "^5.1.5",
    "mustache": "^4.2.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-loki": "^6.0.8",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/eslint": "^8.56.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.4",
    "@types/strip-color": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "body-parser": "^1.20.2",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.6.3",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "strip-color": "^0.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston-transport": "^4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dm112-tadbox/log-vault.git"
  },
  "bugs": {
    "url": "https://github.com/dm112-tadbox/log-vault/issues"
  },
  "homepage": "https://github.com/dm112-tadbox/log-vault#readme"
}
